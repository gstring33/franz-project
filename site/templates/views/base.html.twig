<!DOCTYPE html>
<html lang="en">
<head>
    <!-- Title -->
    <title>Home Default | | Unify - Responsive Website Template</title>

    <!-- Required Meta Tags Always Come First -->
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
    <meta http-equiv="x-ua-compatible" content="ie=edge">

    <!-- Favicon -->
    <link rel="shortcut icon" href="">
    <!-- Google Fonts -->
    <link rel="stylesheet" href="//fonts.googleapis.com/css?family=Open+Sans%3A400%2C300%2C500%2C600%2C700%7CPlayfair+Display%7CRoboto%7CRaleway%7CSpectral%7CRubik">
    <!-- CSS Global Compulsory -->
    <link rel="stylesheet" href="{{ styles('vendors') }}">

    <!-- CSS Unify -->
    <link rel="stylesheet" href="{{ styles('unify-core') }}">
    <link rel="stylesheet" href="{{ styles('unify-components') }}">
    <link rel="stylesheet" href="{{ styles('unify-globals') }}">

    <!-- CSS Customization -->
    <link rel="stylesheet" href="{{ styles('custom') }}">
</head>

<body>
<main>
    <header id="js-header" class="u-header u-header--static">
        {% include './common/header.html.twig' %}
    </header>
    {% block content %}{% endblock %}
    <!-- Footer -->
    <div id="contacts-section" class="g-bg-black-opacity-0_9 g-color-white-opacity-0_8 g-py-60">
        {% include './common/footer.html.twig' %}
    </div>
    <!-- End Footer -->
</main>

<!-- JS Global Compulsory -->
<script src="{{ scripts('vendors') }}"></script>

<!-- JS Unify -->
<script src="{{ scripts('hs.core') }}"></script>

<!-- JS Custom -->
<script src="{{ scripts('custom') }}"></script>
</body>
</html>